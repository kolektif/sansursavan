{
    "manifest_version": 2,
    "name": "Sansürsavan",
    "version": "1.0.1",
    "description": "Sansürlü sitelerin yeni adreslerine yönlendirir.",
    "permissions": [
        "storage",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_icon": "icons/sansursavan-browseraction-g-64.png",
        "default_title": "Sansürsavan vekil sunucu kapalı",
        "browser_style": true
    },
    "icons": {
        "48": "icons/sansursavan-48.png",
        "96": "icons/sansursavan-96.png"
    }
}
