{
    "manifest_version": 2,
    "name": "SansürSavan",
    "version": "1.0.2",
    "description": "Sansürlü sitelerin açılmasına yardımcı olur",
    "permissions": [
        "storage",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ],
    "background": {
        "scripts": [
            "js/background.js"
        ]
    },
    "browser_action": {
        "default_icon": "icons/sansursavan-browseraction-g-64.png",
        "default_title": "SansürSavan",
        "browser_style": true,
        "default_popup": "popup.html"
    },
    "icons": {
        "48": "icons/sansursavan-48.png",
        "96": "icons/sansursavan-96.png"
    }
}
